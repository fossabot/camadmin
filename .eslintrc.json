{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:vue/vue3-recommended", "plugin:vue/base", "prettier"],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2022
  },
  "rules": {
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "semi": ["error", "never"],
    "quotes": ["error", "single"],
    "vue/require-default-prop": "off",
    "vue/no-v-html": "off",
    "vue/multi-word-component-names": "off"
  },
  "globals": {
    "axios": false,
    "route": false
  }
}
