{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:vue/vue3-recommended", "prettier"],
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "semi": ["error", "never"],
    "quotes": ["error", "single"],
    "vue/require-default-prop": "off",
    "vue/no-v-html": "off",
    "vue/multi-word-component-names": "off"
  },
  "globals": {
    "axios": false,
    "route": false
  }
}
